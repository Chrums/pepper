  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js --disableHostCheck & npm run start-socket & npm run start-db",
    "start-prod": "npm run build && node express.js & npm run start-socket & npm run start-db",
    "start-socket": "node ws.js",
    "start-db": "mongod --dbpath=./data",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
